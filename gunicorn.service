[Unit]
Description=demo fastapi application
After=network.target

[Service]
User=fastapi
Group=fastapi
WorkingDirectory=/home/tarunpatnala/app/src 
Environment="PATH=/home/tarunpatnala/app/venv/bin
ExecStart=/home/tarunpatnala/app/venv/bin/gunicorn -w 4 -k uvicorn.workers.UvicornWorker app.main:app --bind 0.0.0.0:800

[Install]
WantedBy=multi-user.target